  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "typescript": "4.7.4",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "next": "^13.0.7",
